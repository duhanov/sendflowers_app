<%admin_page @tabs do%>
	<%table_with_sort_links [["Язык", "language"], ["Дата", "holiday_month,holiday_day"], ["Название", "name"], ["Тип", "t"], "Ред.", "Уд."], {:del_url => "/#{params[:controller]}/delete/", :item_name => model_title[:one], :update_url => "/#{params[:controller]}/update/"} do%>
		<%for item in @items%>
			<%tr item do%>
			<td><%=item.language%></td>
			<td><%=holiday_date item%></td>
			<td class="edit_string2" field="name"><%=item.name%></td>
			<td><%=holiday_type item%></td>
			<td><%=btn_edit item%></td>
			<td><%=btn_del%></td>
			<%end%>
		<%end%>
	<%end%>
	<%=will_paginate @items%>
<%end%>
